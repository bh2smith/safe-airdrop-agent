{
  "name": "airdrop-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "agent": "concurrently \"tsx src/index.ts\" \"make-agent dev\"",
    "api": "tsx src/index.ts",
    "build": "tsc",
    "deploy": "make-agent deploy -u https://safe-airdrop-agent.vercel.app/",
    "lint": "eslint {src,tests}/**/*.ts && prettier --check '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "eslint --fix {src,tests}/**/*.ts && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.4.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multi-asset-transfer": "^0.1.3",
    "papaparse": "^5.5.3",
    "viem": "^2.37.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/papaparse": "^5.3.16",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "make-agent": "0.4.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
